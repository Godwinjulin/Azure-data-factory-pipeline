{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ubaadf"
		},
		"AzureSqlDatabase1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSqlDatabase1'"
		},
		"AzureSqlDatabase2_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSqlDatabase2'"
		},
		"AzureSqlDatabase1_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "uba-serv.database.windows.net"
		},
		"AzureSqlDatabase1_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "uba-db"
		},
		"AzureSqlDatabase1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "ubaadmin"
		},
		"AzureSqlDatabase2_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "uba-ser2.database.windows.net"
		},
		"AzureSqlDatabase2_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "ubadb2"
		},
		"AzureSqlDatabase2_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "ubaadmin"
		},
		"AzureSqlDatabase336_properties_privateLinkResourceId": {
			"type": "string",
			"defaultValue": "/subscriptions/4be40ab3-4495-4f7d-bb7c-148801189940/resourceGroups/rg-uba/providers/Microsoft.Sql/servers/uba-ser2"
		},
		"AzureSqlDatabase336_properties_groupId": {
			"type": "string",
			"defaultValue": "sqlServer"
		},
		"AzureSqlDatabase336_properties_fqdns": {
			"type": "array",
			"defaultValue": [
				"uba-ser2.database.windows.net"
			]
		},
		"AzureSqlDatabase354_properties_privateLinkResourceId": {
			"type": "string",
			"defaultValue": "/subscriptions/4be40ab3-4495-4f7d-bb7c-148801189940/resourceGroups/rg-uba/providers/Microsoft.Sql/servers/uba-serv"
		},
		"AzureSqlDatabase354_properties_groupId": {
			"type": "string",
			"defaultValue": "sqlServer"
		},
		"AzureSqlDatabase354_properties_fqdns": {
			"type": "array",
			"defaultValue": [
				"uba-serv.database.windows.net"
			]
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ubaadf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSqlDatabase1_password": {
						"value": "[parameters('AzureSqlDatabase1_password')]"
					},
					"AzureSqlDatabase2_password": {
						"value": "[parameters('AzureSqlDatabase2_password')]"
					},
					"AzureSqlDatabase336_properties_privateLinkResourceId": {
						"value": "[parameters('AzureSqlDatabase336_properties_privateLinkResourceId')]"
					},
					"AzureSqlDatabase336_properties_groupId": {
						"value": "[parameters('AzureSqlDatabase336_properties_groupId')]"
					},
					"AzureSqlDatabase336_properties_fqdns": {
						"value": "[parameters('AzureSqlDatabase336_properties_fqdns')]"
					},
					"AzureSqlDatabase354_properties_privateLinkResourceId": {
						"value": "[parameters('AzureSqlDatabase354_properties_privateLinkResourceId')]"
					},
					"AzureSqlDatabase354_properties_groupId": {
						"value": "[parameters('AzureSqlDatabase354_properties_groupId')]"
					},
					"AzureSqlDatabase354_properties_fqdns": {
						"value": "[parameters('AzureSqlDatabase354_properties_fqdns')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_server')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_database')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_userName": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_userName')]"
					},
					"AzureSqlDatabase2_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlDatabase2_properties_typeProperties_server')]"
					},
					"AzureSqlDatabase2_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlDatabase2_properties_typeProperties_database')]"
					},
					"AzureSqlDatabase2_properties_typeProperties_userName": {
						"value": "[parameters('AzureSqlDatabase2_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}